{
    "name": "setono/google-analytics-measurement-protocol",
    "type": "library",
    "description": "A library that eases the generation of measurement protocol urls",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": "^7.4",
        "php-http/discovery": "^1.13",
        "psr/http-client": "^1.0",
        "psr/http-client-implementation": "^1",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "^1",
        "thecodingmachine/safe": "^1.3"
    },
    "require-dev": {
        "kriswallsmith/buzz": "^1.2",
        "nyholm/psr7": "^1.3",
        "phpunit/phpunit": "^9.5",
        "setono/code-quality-pack": "^2.0",
        "symfony/http-foundation": "^4.4 || ^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\GoogleAnalyticsMeasurementProtocol\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\GoogleAnalyticsMeasurementProtocol\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": [
            "phpstan analyse",
            "psalm"
        ],
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
