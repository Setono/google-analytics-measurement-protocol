{
    "name": "setono/google-analytics-measurement-protocol",
    "description": "A library that eases the generation of measurement protocol payloads",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "php-http/discovery": "^1.18",
        "psr/http-client": "^1.0",
        "psr/http-client-implementation": "^1",
        "psr/http-factory": "^1.0",
        "psr/http-factory-implementation": "^1",
        "psr/http-message": "^1.0 || ^2.0",
        "psr/log": "^1.0 || ^2.0 || ^3.0",
        "setono/google-analytics-events": "^1.0@rc",
        "symfony/polyfill-php80": "^1.27"
    },
    "require-dev": {
        "infection/infection": "^0.26",
        "nyholm/psr7": "^1.8",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-phpunit": "^0.18",
        "setono/code-quality-pack": "^2.4",
        "symfony/http-client": "^5.4 || ^6.0"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Setono\\GoogleAnalyticsMeasurementProtocol\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\GoogleAnalyticsMeasurementProtocol\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false,
            "ergebnis/composer-normalize": true,
            "infection/extension-installer": true,
            "php-http/discovery": false
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "scripts": {
        "analyse": "psalm",
        "check-style": "ecs check",
        "fix-style": "ecs check --fix",
        "phpunit": "phpunit"
    }
}
